---
import "../styles/global.css";
import { SEO } from "astro-seo";
import Favicons from "./global/Favicons.astro";
---
<SEO
  title="Zenex Studio"
  description="We create web applications that match your business needs."
  canonical="https://www.zenex.dev"
  openGraph={{
    basic: {
      title: "Zenex Studio",
      type: "website",  // 'website' is a common type for company sites
      image: "https://zenex.dev/favicon.png",  // Update with your actual image link
    }
  }}
  twitter={{
    creator: "@raulcanodev"
  }}
  extend={{
    link: [{ rel: "icon", href: "/favicon.png" }],
    meta: [
      {
        name: "twitter:image",
        content: "https://zenex.dev/favicon.png",  // Update with your actual image link
      },
      { name: "twitter:title", content: "Zenex Studio" },
      { name: "twitter:description", content: "Explore web applications built to match your business needs." },
    ],
  }}
/>
